<% @entry.each do |ent| %>
  <div class="hot-blog col-md-5">
    <h5 class="title24 dosis-font font-bold"><%=link_to ent.title, entry_path(ent) %></h5>
    <%= image_tag ent.image if ent.image.attached?%>
    <%= link_to t(".read_more"), entry_path(ent), class: "btn btn-success" %>
  </div>
<% end %>
